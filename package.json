{
  "name": "@manageiq/react-ui-components",
  "version": "0.0.1",
  "description": "React based ui components for ManageIQ project.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config scripts/webpack.config.js --watch",
    "build": "webpack --config scripts/webpack.config.js",
    "vendor": "webpack --config scripts/webpack.config.js --env.build",
    "server": "webpack-dev-server --config scripts/webpack.config.js --env.server",
    "test": "jest",
    "doc": "react-docgen",
    "storybook": "start-storybook -c scripts/storybook -p 6006",
    "build-storybook": "build-storybook",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.8.1"
  },
  "files": [
    "dist/",
    "!dist/vendor.js",
    "!dist/vendor.css"
  ],
  "keywords": [
    "react",
    "manageiq",
    "components",
    "ui"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*",
      "!src/**/vendor.js"
    ],
    "setupFiles": [
      "<rootDir>/scripts/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-info": "^3.3.13",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addon-options": "^3.3.13",
    "@storybook/addons": "^3.3.12",
    "@storybook/react": "^3.3.13",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.0.0",
    "copy-webpack-plugin": "^4.3.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.1",
    "jquery": "^3.3.1",
    "mkdir": "0.0.2",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "patternfly-react": "^1.8.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "prop-types": "^15.6.0",
    "react-props": "0.0.3",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "react-redux": "^5.0.6"
  }
}
